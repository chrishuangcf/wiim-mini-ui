        "vuetify": "^3.0.0-beta.0",
        "webfontloader": "^1.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@babel/eslint-parser": "^7.12.16",
        "@mdi/font": "^6.9.96",
        "@rushstack/eslint-patch": "^1.1.0",
        "@types/node": "^16.11.41",
        "@vitejs/plugin-vue": "^2.3.3",
        "@vitejs/plugin-vue-jsx": "^1.3.10",
